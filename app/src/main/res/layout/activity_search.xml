<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    tools:context=".Activity.SearchActivity">

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:background="@android:color/transparent"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/searchBarBackButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_arrow_back_black_24dp"
            app:tint="@color/color_primary_text"
            tools:ignore="ContentDescription" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/explore_box_design"
            android:layout_margin="10dp"
            android:gravity="center"
            android:layout_marginStart="@dimen/margin_10"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView">

            <SearchView
                android:id="@+id/searchFilm"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:backgroundTintMode="screen"
                android:iconifiedByDefault="true"
                android:queryHint="Search Blog"
                android:theme="@style/SearchView"
                app:queryBackground="@android:color/transparent"
                tools:ignore="RtlSymmetry" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/view9"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/whiteIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view9">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Recent Searches"
            android:textColor="@color/color_primary_text"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:textStyle="bold"
            android:textSize="20sp"
            android:layout_marginStart="20dp"
            tools:ignore="HardcodedText" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/whiteIcon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout13" />

        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:focusable="true"
            android:clickable="true"
            android:gravity="center">

            <ImageView
                android:id="@+id/icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/history"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/search1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@id/icon"
                android:layout_centerVertical="true"
                android:layout_marginStart="30dp"
                android:text="jhc akhcka "
                android:textColor="@color/color_primary_text"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/cross1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:src="@drawable/ic_baseline_close_24"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:id="@+id/icon2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/history"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/search2"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@id/icon2"
                android:layout_centerVertical="true"
                android:layout_marginStart="30dp"
                android:text="jhc akhcka "
                android:textColor="@color/color_primary_text"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/cross2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:src="@drawable/ic_baseline_close_24"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:id="@+id/icon3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/history"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/search3"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@id/icon3"
                android:layout_centerVertical="true"
                android:layout_marginStart="30dp"
                android:text="jhc akhcka "
                android:textColor="@color/color_primary_text"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/cross3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:src="@drawable/ic_baseline_close_24"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:id="@+id/icon4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/history"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/search4"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@id/icon4"
                android:layout_centerVertical="true"
                android:layout_marginStart="30dp"
                android:text="jhc akhcka "
                android:textColor="@color/color_primary_text"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/cross4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:src="@drawable/ic_baseline_close_24"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">

            <ImageView
                android:id="@+id/icon5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="40dp"
                android:layout_centerVertical="true"
                android:src="@drawable/history"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/search5"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_alignStart="@id/icon5"
                android:layout_centerVertical="true"
                android:layout_marginStart="30dp"
                android:text="jhc akhcka "
                android:textColor="@color/color_primary_text"
                tools:ignore="HardcodedText" />

            <ImageView
                android:id="@+id/cross5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="40dp"
                android:src="@drawable/ic_baseline_close_24"
                app:tint="@color/color_primary_text"
                tools:ignore="ContentDescription" />

        </RelativeLayout>


    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/blogRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout13" />


</androidx.constraintlayout.widget.ConstraintLayout>